#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/cli/esm/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/cli/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/cli/esm/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/cli/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules/@graphql-mesh/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-mesh+cli@0.100.13_graphql@16.11.0_ioredis@5.7.0/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../esm/bin.js" "$@"
else
  exec node  "$basedir/../../esm/bin.js" "$@"
fi
