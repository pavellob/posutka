#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/gateway/dist/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/gateway/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/gateway/dist/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/gateway/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules/@graphql-hive/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/@graphql-hive+gateway@2.1.2_@types+ioredis-mock@8.2.6_ioredis@5.7.0__@types+node@22.18._79b28a016cd9f5fb05cb61dfe6e146e4/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-hive/gateway/dist/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-hive/gateway/dist/bin.js" "$@"
fi
