#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/thriftrw@3.11.4/node_modules/thriftrw/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/thriftrw@3.11.4/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/thriftrw@3.11.4/node_modules/thriftrw/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/thriftrw@3.11.4/node_modules:/Users/pavellobachev/dev/posutka-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../thriftrw/thrift2json.js" "$@"
else
  exec node  "$basedir/../thriftrw/thrift2json.js" "$@"
fi
